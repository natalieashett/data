# Simple exploration of world database
print("=== EXPLORING WORLD DATABASE ===")

# Read the SQL file and show first 30 lines
with open('world db (1).sql', 'r', encoding='utf-8') as file:
    lines = file.readlines()

print(f"Total lines in file: {len(lines)}")
print("\nFirst 30 lines:")
print("-" * 50)

for i, line in enumerate(lines[:30]):
    print(f"{i+1:2d}: {line.strip()}")

# Look for table creation
print("\n=== LOOKING FOR TABLES ===")
for i, line in enumerate(lines):
    if 'CREATE TABLE' in line.upper():
        print(f"Line {i+1}: {line.strip()}")
        # Show next few lines too
        for j in range(1, 5):
            if i+j < len(lines):
                print(f"Line {i+j+1}: {lines[i+j].strip()}")
        print("-" * 30)

# Look for data insertion
print("\n=== LOOKING FOR DATA ===")
insert_count = 0
for i, line in enumerate(lines):
    if 'INSERT INTO' in line.upper() and insert_count < 5:
        print(f"Line {i+1}: {line.strip()[:100]}...")
        insert_count += 1
